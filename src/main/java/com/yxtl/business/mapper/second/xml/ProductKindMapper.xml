<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yxtl.business.mapper.second.ProductKindMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yxtl.business.entity.second.ProductKind">
        <id column="id" property="id" />
        <result column="name" property="name" />
    </resultMap>

    <select id="showKind" resultType="com.yxtl.business.entity.second.ProductKind">
        SELECT * FROM product_kind ORDER BY id DESC
    </select>

</mapper>
